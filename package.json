{
  "name": "browk-ui-kit",
  "description": "A React component library based on the Browk UI Kit.",
  "author": "evfjunior <evfjunior@protonmail.com>",
  "private": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/evfjunior/browk-ui-kit.git"
  },
  "bugs": {
    "url": "https://github.com/evfjunior/browk-ui-kit/issues"
  },
  "homepage": "https://github.com/evfjunior/browk-ui-kit#readme",
  "keywords": [
    "react",
    "typescript",
    "component",
    "library",
    "ui",
    "kit",
    "styled-components",
    "storybook"
  ],
  "scripts": {
    "cm": "cz",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "gen:component": "plop",
    "lint": "eslint src --max-warnings=0",
    "prepare": "husky install",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "test": "jest --maxWorkers=50% --passWithNoTests",
    "test:ci": "jest --ci --colors --passWithNoTests",
    "test:watch": "jest --maxWorkers=25% --watch",
    "typecheck": "tsc --project tsconfig.json --noEmit"
  },
  "dependencies": {
    "@storybook/addon-docs": "^6.5.13",
    "@storybook/cli": "^6.5.9",
    "next": "12.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-is": "^18.2.0",
    "styled-components": "^5.3.6",
    "styled-icons": "^10.46.0"
  },
  "devDependencies": {
    "@babel/core": "^7.19.0",
    "@commitlint/cli": "^17.0.0",
    "@commitlint/config-conventional": "^17.0.0",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-interactions": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/addons": "^6.5.9",
    "@storybook/api": "^6.5.9",
    "@storybook/builder-webpack5": "^6.5.9",
    "@storybook/components": "^6.5.9",
    "@storybook/core-events": "^6.5.9",
    "@storybook/manager-webpack5": "^6.5.9",
    "@storybook/react": "^6.5.9",
    "@storybook/testing-library": "^0.0.13",
    "@storybook/theming": "^6.5.9",
    "@testing-library/dom": "^8.17.1",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/node": "18.7.16",
    "@types/react": "18.0.14",
    "@types/react-dom": "18.0.5",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "babel-loader": "^8.2.5",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "8.23.1",
    "eslint-config-next": "12.3.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.8",
    "husky": "^8.0.1",
    "jest": "^29.0.3",
    "jest-environment-jsdom": "^29.0.3",
    "jest-styled-components": "^7.1.1",
    "lint-staged": "^13.0.3",
    "plop": "^3.1.0",
    "prettier": "^2.7.1",
    "storybook-dark-mode": "^1.1.0",
    "typescript": "4.8.3"
  },
  "resolutions": {
    "styled-components": "^5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
